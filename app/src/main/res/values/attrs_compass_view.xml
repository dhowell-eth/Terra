<resources>
    <declare-styleable name="CompassView">
        <attr name="baseImage" format="reference" />
        <attr name="disabledImage" format="reference"/>
        <attr name="orientationModeImage" format="reference"/>
        <attr name="needleLengthProportion" format="float"/>
        <attr name="needleImage" format="reference"/>
        <attr name="needleMode">
            <enum name="needle" value="1"/>
            <enum name="vector" value="2"/>
            <enum name="plane" value="3"/>
        </attr>
        <attr name="needleColor" format="color|reference"/>
        <attr name="needleWidth" format="float"/>
        <attr name="isEnabled" format="boolean"/>
    </declare-styleable>
</resources>
